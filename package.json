    "service-worker": "mkdir -p docs && browserify ./client/service-worker.js > ./docs/service-worker.js"
  },
  "author": "Simon McManus",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^16.0.0",
    "cypress": "^3.3.1",
    "jquery": "3.x.x",
    "speclate": "^16.1.2",
    "speclate-router": "^8.3.0",
    "speclate-service-worker": "1.x.x"
  }
}
